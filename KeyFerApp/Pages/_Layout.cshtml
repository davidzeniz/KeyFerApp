    <script>
        function confirmExit() {
            // Display the confirmation message
            var confirmationMessage = "¿Estás seguro de que quieres salir de la App?";
            if (confirm(confirmationMessage)) {
                // User confirmed, proceed with the link action
                return true;
            } else {
                // User canceled, prevent the default anchor link behavior
                return false;
            }
        }
    </script>

